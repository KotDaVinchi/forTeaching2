doctype html
- var links = ["Lorem", "ipsum", "dolor", "sit", "ame"]
html
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width,initial-scale=1")
    title=title
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    div.header
      each link in links
        a(href=link) #{link}
    div.container
      block content
